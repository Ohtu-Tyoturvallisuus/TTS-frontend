{
  "cli": {
    "version": ">= 10.8.2",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "intentionally_left_blank",
        "ascAppId": "$intentionally_left_blank",
        "appleTeamId": "$intentionally_left_blank"
      }
    }
  }
}
