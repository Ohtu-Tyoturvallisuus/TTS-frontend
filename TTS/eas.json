{
  "cli": {
    "version": ">= 10.8.2",
    "appVersionSource": "remote"
  },
  "build": {
    "main": {
      "autoIncrement": true,
      "channel": "main"
    },
    "uat": {
      "autoIncrement": true,
      "channel": "uat"
    },
    "production": {
      "autoIncrement": true,
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "intentionally_left_blank",
        "ascAppId": "intentionally_left_blank",
        "appleTeamId": "intentionally_left_blank"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "draft",
        "changesNotSentForReview": true
      }
    },
    "uat": {
      "ios": {
        "appleId": "intentionally_left_blank",
        "ascAppId": "intentionally_left_blank",
        "appleTeamId": "intentionally_left_blank"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "draft",
        "changesNotSentForReview": true
      }
    },
    "main": {
      "ios": {
        "appleId": "intentionally_left_blank",
        "ascAppId": "intentionally_left_blank",
        "appleTeamId": "intentionally_left_blank"
      },
      "android": {
        "track": "internal",
        "releaseStatus": "draft",
        "changesNotSentForReview": true
      }
    }
  }
}
